{
 "ignore": [
  ".env",
  ".eslintignore",
  ".git",
  ".github/ISSUE_TEMPLATE",
  "coverage",
  "cypress",
  ".idea",
  ".husky",
  ".yarn",
  ".nvmrc",
  ".yarnrc",
  "yarn-error.log",
  "**/*.json",
  "**/*.svg",
  "**/*.png",
  "**/*.txt",
  "**/*.md",
  "**/.prettierrc",
  "**/.prettierignore",
  "**/OWNERS",
  "**/*.lock",
  "**/.DS_Store"
 ],
 "license": "license-header.txt",
 "defaultFormat": {
  "eachLine": {
   "prepend": "# "
  },
  "append": ""
 },
 "licenseFormats": {
  "ts|js": {
   "eachLine": {
    "prepend": " * "
   },
   "prepend": "/*",
   "append": " */"
  }
 },
 "trailingWhitespace": "TRIM"
}
