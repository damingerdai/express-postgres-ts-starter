{
 "name": "express-postgres-ts-starter",
 "version": "0.2.0",
 "description": "express-postgres-ts-starter",
 "main": "server.ts",
 "engines": {
  "node": "^12.0.0 || ^14.0.0 || ^16.0.0",
  "yarn": ">=1.22.4 <2",
  "npm": "Please use Yarn instead of NPM to install dependencies. See: https://yarnpkg.com/lang/en/docs/install/"
 },
 "scripts": {
  "build": "webpack",
  "build:watch": "webpack --watch",
  "esbuild": "./node_modules/.bin/esbuild server.ts --bundle --outfile=dist/esbuild/server.js --platform=node --target=node14.15.3",
  "test": "jest",
  "check:license": "license-check-and-add check",
  "changelog": "npx conventional-changelog -p angular -i CHANGELOG.md -s",
  "changelog:all": "npx conventional-changelog -p angular -i CHANGELOG.md -w -s 0",
  "migrate:make": "./node_modules/.bin/knex --knexfile db/knexfile.js migrate:make $@",
  "migrate:up": "./node_modules/.bin/knex --knexfile db/knexfile.js migrate:latest",
  "migrate:up:local": "cross-env POSTGRES_HOST=localhost POSTGRES_PORT=5432 yarn migrate:up",
  "migrate:down": "./node_modules/.bin/knex --knexfile db/knexfile.js migrate:rollback",
  "migrate:down:local": "cross-env POSTGRES_HOST=localhost ./node_modules/.bin/knex --knexfile db/knexfile.js migrate:rollback",
  "seed:make": "./node_modules/.bin/knex --knexfile db/knexfile.js seed:make",
  "seed:run:local": "cross-env POSTGRES_HOST=localhost ./node_modules/.bin/knex --knexfile db/knexfile.js seed:run",
  "start:dev": "ts-node-dev --require tsconfig-paths/register server.ts",
  "prettier": "prettier --check src/**/*.ts",
  "eslint:ts": "eslint src/**/*.ts",
  "lint": "yarn eslint:ts && yarn check:license",
  "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
  "version:patch": "yarn version --patch",
  "version:minor": "yarn version --minor",
  "prepare": "husky install"
 },
 "keywords": [
  "express",
  "postgres",
  "typescript"
 ],
 "author": "arthur ming",
 "license": "Apache-2.0",
 "dependencies": {
  "apollo-server-express": "^2.17.0",
  "body-parser": "^1.19.0",
  "cookie-parser": "^1.4.5",
  "express": "^4.17.1",
  "express-joi-validation": "^0.2.1",
  "express-promise-router": "^3.0.3",
  "graphql": "^15.5.1",
  "graphql-tag": "^2.11.0",
  "helmet": "^4.4.1",
  "joi": "^14.3.1",
  "knex": "^0.95.6",
  "lodash": "^4.17.21",
  "pg": "^8.0.3"
 },
 "devDependencies": {
  "@commitlint/cli": "^12.1.4",
  "@commitlint/config-conventional": "^11.0.0",
  "@types/express": "^4.17.6",
  "@types/helmet": "^0.0.47",
  "@types/jest": "^26.0.24",
  "@types/joi": "^14.3.4",
  "@typescript-eslint/eslint-plugin": "^3.9.0",
  "@typescript-eslint/parser": "^3.9.0",
  "conventional-changelog-cli": "^2.0.31",
  "cross-env": "^7.0.3",
  "esbuild": "^0.12.15",
  "eslint": "^7.19.0",
  "eslint-config-prettier": "^6.11.0",
  "husky": "^7.0.1",
  "jest": "^27.0.3",
  "license-check-and-add": "^4.0.2",
  "lint-staged": "^11.0.0",
  "prettier": "^2.3.2",
  "ts-jest": "^27.0.2",
  "ts-loader": "^9.2.6",
  "ts-node": "^9.0.0",
  "ts-node-dev": "^1.1.6",
  "tsconfig-paths": "^3.9.0",
  "tsconfig-paths-webpack-plugin": "^3.2.0",
  "typescript": "4.4.4",
  "webpack": "^5.58.2",
  "webpack-cli": "^4.9.0",
  "webpack-node-externals": "^2.5.2"
 },
 "resolutions": {
  "**/**/glob-parent": ">=5.1.2",
  "**/**/ws": ">=5.2.3 <6.0.0 || >=6.2.2 <7.0.0 || >=7.4.6",
  "**/**/trim-newlines": ">=3.0.1 <4.0.0 || >=4.0.1"
 }
}
