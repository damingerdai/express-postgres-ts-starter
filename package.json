{
  "name": "express-postgres-ts-starter",
  "version": "0.0.1",
  "description": "express-postgres-ts-starter",
  "main": "server.ts",
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack --watch",
    "test": "jest",
    "changelog": "npx conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:all": "npx conventional-changelog -p angular -i CHANGELOG.md -w -s 0",
    "migrate:make": "./node_modules/.bin/knex --knexfile db/knexfile.js migrate:make $@",
    "migrate:up": "./node_modules/.bin/knex --knexfile db/knexfile.js migrate:latest",
    "migrate:down": "./node_modules/.bin/knex --knexfile db/knexfile.js migrate:rollback",
    "seed:make": "./node_modules/.bin/knex --knexfile db/knexfile.js seed:make",
    "start:dev": "ts-node-dev --require tsconfig-paths/register server.ts"
  },
  "keywords": [
    "express",
    "postgres",
    "typescript"
  ],
  "author": "arthur ming",
  "license": "Apache-2.0",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-joi-validation": "^0.2.1",
    "express-promise-router": "^3.0.3",
    "joi": "^14.3.1",
    "knex": "^0.21.1",
    "lodash": "^4.17.19",
    "pg": "^8.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/express": "^4.17.6",
    "@types/helmet": "^0.0.47",
    "@types/jest": "^26.0.3",
    "@types/joi": "^14.3.4",
    "awesome-typescript-loader": "^5.2.1",
    "conventional-changelog-cli": "^2.0.31",
    "helmet": "^3.22.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.8.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "resolutions": {
    "**/**/dot-prop": ">=5.1.1",
    "**/**/yargs-parser": ">=13.1.2 <14.0.0 || >=15.0.1 <16.0.0 || >=18.1.2",
    "**/**/lodash": ">=4.17.19"
  }
}
