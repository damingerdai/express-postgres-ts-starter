{
 "name": "express-postgres-ts-starter",
 "version": "0.4.1",
 "description": "express-postgres-ts-starter",
 "main": "server.ts",
 "engines": {
  "node": "^18.0.0 || ^20.0.0",
  "yarn": ">=1.22.4 <2",
  "npm": "Please use Yarn instead of NPM to install dependencies. See: https://yarnpkg.com/lang/en/docs/install/"
 },
 "scripts": {
  "tsc": "tsc",
  "build": "webpack",
  "build:watch": "webpack --watch",
  "build:bazel": "bazel build //:server",
  "esbuild": "./node_modules/.bin/esbuild server.ts --bundle --outfile=dist/esbuild/server.js --platform=node --target=node14.15.3",
  "test": "jest --runInBand --detectOpenHandles --forceExit",
  "test:ci": "jest --runInBand --detectOpenHandles --forceExit --coverage",
  "check:license": "license-check-and-add check",
  "check:license:fix": "license-check-and-add add",
  "changelog": "npx conventional-changelog -p angular -i CHANGELOG.md -s",
  "changelog:all": "npx conventional-changelog -p angular -i CHANGELOG.md -w -s 0",
  "migrate:make": "./node_modules/.bin/knex --knexfile db/knexfile.js migrate:make $@",
  "migrate:up": "./node_modules/.bin/knex --knexfile db/knexfile.js migrate:latest",
  "migrate:up:local": "cross-env POSTGRES_HOST=localhost POSTGRES_PORT=5432 yarn migrate:up",
  "migrate:down": "./node_modules/.bin/knex --knexfile db/knexfile.js migrate:rollback",
  "migrate:down:local": "cross-env POSTGRES_HOST=localhost ./node_modules/.bin/knex --knexfile db/knexfile.js migrate:rollback",
  "seed:make": "./node_modules/.bin/knex --knexfile db/knexfile.js seed:make",
  "seed:run": "./node_modules/.bin/knex --knexfile db/knexfile.js seed:run",
  "seed:run:local": "cross-env POSTGRES_HOST=localhost ./node_modules/.bin/knex --knexfile db/knexfile.js seed:run",
  "start:dev": "ts-node-dev --require tsconfig-paths/register server.ts",
  "start:nodemon": "nodemon",
  "prettier": "prettier --check src/**/*.ts",
  "eslint:ts": "eslint . --ext .js,.jsx,.ts,.tsx",
  "lint": "yarn eslint:ts && yarn check:license",
  "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
  "version:patch": "yarn version --patch",
  "version:minor": "yarn version --minor",
  "prepare": "is-ci || husky install",
  "postinstall": "patch-package"
 },
 "keywords": [
  "express",
  "postgres",
  "typescript"
 ],
 "author": "arthur ming",
 "license": "Apache-2.0",
 "dependencies": {
  "@apollo/server": "^4.10.1",
  "@graphql-tools/merge": "^9.0.0",
  "body-parser": "^1.20.2",
  "compression": "^1.7.4",
  "cookie-parser": "^1.4.5",
  "dotenv": "^16.1.4",
  "exceljs": "^4.3.0",
  "express": "^4.18.2",
  "express-fileupload": "^1.4.0",
  "express-joi-validation": "^0.3.0",
  "express-session": "^1.17.3",
  "graphql": "^16.8.1",
  "graphql-tag": "^2.12.6",
  "helmet": "^7.0.0",
  "joi": "^17.9.2",
  "jsonwebtoken": "^9.0.0",
  "knex": "^2.4.2",
  "lodash": "^4.17.21",
  "minio": "^7.1.1",
  "morgan": "^1.10.0",
  "nodemon": "^3.1.0",
  "pg": "^8.10.0",
  "pg-boss": "^9.0.1",
  "redis": "^4.6.7",
  "socket.io": "^4.6.2",
  "winston": "^3.9.0"
 },
 "devDependencies": {
  "@bazel/bazelisk": "^1.7.5",
  "@bazel/buildifier": "^6.0.0",
  "@bazel/ibazel": "^0.25.0",
  "@bazel/typescript": "^5.8.1",
  "@commitlint/cli": "^17.6.5",
  "@commitlint/config-conventional": "^17.6.5",
  "@types/express": "^4.17.21",
  "@types/express-fileupload": "^1.4.1",
  "@types/express-session": "^1.17.7",
  "@types/helmet": "^0.0.48",
  "@types/jest": "^29.5.12",
  "@types/joi": "^17.2.3",
  "@types/minio": "^7.0.18",
  "@types/node": "^20.3.1",
  "@typescript-eslint/eslint-plugin": "^7.1.0",
  "@typescript-eslint/parser": "^7.1.0",
  "conventional-changelog-cli": "^3.0.0",
  "cross-env": "^7.0.3",
  "esbuild": "^0.20.0",
  "eslint": "^8.57.0",
  "eslint-config-prettier": "^9.1.0",
  "husky": "^8.0.3",
  "is-ci": "^3.0.1",
  "jest": "^29.7.0",
  "license-check-and-add": "^4.0.5",
  "lint-staged": "^13.2.2",
  "patch-package": "^8.0.0",
  "prettier": "^3.0.0",
  "ts-jest": "^29.1.0",
  "ts-loader": "^9.4.2",
  "ts-node": "^10.8.1",
  "ts-node-dev": "^2.0.0",
  "tsconfig-paths": "^4.2.0",
  "tsconfig-paths-webpack-plugin": "^4.0.1",
  "typescript": "5.4.5",
  "webpack": "^5.82.1",
  "webpack-cli": "^5.1.0",
  "webpack-node-externals": "^3.0.0"
 },
 "resolutions": {
  "**/**/glob-parent": ">=5.1.2",
  "**/**/ws": ">=5.2.3 <6.0.0 || >=6.2.2 <7.0.0 || >=7.4.6",
  "**/**/trim-newlines": ">=3.0.1 <6.0.0"
 }
}
